{{ $z_pages := slice }}
{{ range $p := .Site.RegularPages }}
  {{ if (in $p.RelPermalink "zettelkasten") }}
    {{ $z_pages = $z_pages | append $p }}
  {{ end }}
{{ end }}

{{ $pages := slice }}
{{ range $p := $z_pages }}
  {{ $splits := split $p.RawContent " " }}
  {{ $links := slice }}
  {{ range $s := $splits }}
    {{ if (in $s "zettelkasten") }}
      {{ $links = $links | append (index (split $s "\"") 1) }}
    {{ end }}
  {{ end }}
  {{ $pages = $pages | append (dict "title" $p.Title "links" $links )}}
{{ end }}

{{ $pages | jsonify }}
